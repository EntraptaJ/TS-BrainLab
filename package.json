    "cls-rtracer": "^2.5.1",
    "fastify": "^3.8.0",
    "hyperid": "^2.0.5",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "supports-color": "^5.5.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.29",
    "typeorm-typedi-extensions": "^0.2.3"
  },
  "devDependencies": {
    "@k-foss/ts-esnode": "^1.7.0",
    "@types/bull": "^3.14.4",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.8.2",
